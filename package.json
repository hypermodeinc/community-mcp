{
  "name": "@hypermode/community-mcp",
  "version": "0.0.0-alpha.0",
  "description": "Curated list of new, forked and redesigned MCP servers to be used in conversational agents",
  "packageManager": "pnpm@10.6.0",
  "scripts": {
    "clean": "find . -type d \\( -name \"node_modules\" -o -name \"build\" -o -name \"dist\" \\) -exec rm -rf {} + && rm ./pnpm-lock.yaml",
    "build": "pnpm -r build",
    "start:attio": "pnpm --filter @hypermode/attio-mcp-server start"
  },
  "private": true,
  "homepage": "https://github.com/hypermodeinc/community-mcp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hypermodeinc/community-mcp.git"
  },
  "keywords": [
    "mcp",
    "hypermode",
    "community",
    "server"
  ],
  "author": "Hypermode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hypermodeinc/community-mcp/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.27.2",
    "@babel/core": "7.27.4",
    "@babel/preset-env": "7.27.2",
    "@babel/preset-typescript": "7.27.1",
    "@babel/runtime": "7.27.4",
    "@jest/globals": "29.7.0",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.0.0",
    "@types/node": "22.4.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "jest": "^29.0.0",
    "next": "^15.3.1",
    "typescript": "5.8.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "typescript",
        "ts-node",
        "@tsconfig/*",
        "@types/*",
        "jest",
        "@jest/*",
        "supertest",
        "prettier",
        "webpack"
      ]
    }
  },
  "dependencies": {
    "@hey-api/openapi-ts": "^0.77.0",
    "@types/express": "^5.0.3",
    "express": "^5.1.0"
  }
}
